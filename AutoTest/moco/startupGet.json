[
  {
    "description":"模拟一个没有参数的Get请求",
    "request":{
      "uri":"/getdemo",
      "method":"get"
    },
    "response":{
      "text":"这是一个没有参数的Get请求",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  },
  {
    "description":"模拟一个带参数的Get请求",
    "request":{
      "uri":"/getwithparam",
      "method":"get",
      "queries":{
        "name":"zhangsan",
        "age":"18"
      }
    },
    "response":{
      "text":"张三回来啦",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  }
]